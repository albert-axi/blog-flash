
<h1>Categories Index </h1>
<%= link_to "Add Post", new_category_path %>
<ul>
  <% @categories.each do |category| %>
    <li>
      <%= category.name %>
      <%= link_to "Show", category %> 
      <%= link_to "Edit", edit_category_path(category) %>
      <%= link_to "Delete", category, method: :delete %>
    </li>
  <% end %>
</ul>